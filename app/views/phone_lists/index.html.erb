<p style="color: green"><%= notice %></p>

<h1>Phone lists</h1>

<%= form_tag phone_lists_path, method: :get, remote: true, id: "search-form" do %>
  <div class="field">
    <%= text_field_tag :search, params[:search], placeholder: "Search phone lists..." %>
  </div>
  <div class="actions">
    <%= submit_tag "Search", name: nil, class: "btn btn-primary" %>
  </div>
<% end %>


<div id="phone_lists">
  <% @phone_lists.each do |phone_list| %>
    <table>
      <tr>
        <td><%= render phone_list %></td>
        <% if user_signed_in? && current_user.username == 'admin' %>
          <td><%= link_to "Show this phone list", phone_list %></td>
        <% end %>
      </tr>
    </table>
  <% end %>
</div>


</br>
<%= link_to "Back", links_path %> 



